{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "v8app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/v8app",
      "sourceRoot": "apps/v8app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/v8app",
            "index": "apps/v8app/src/index.html",
            "main": "apps/v8app/src/main.ts",
            "polyfills": "apps/v8app/src/polyfills.ts",
            "tsConfig": "apps/v8app/tsconfig.app.json",
            "assets": [
              "apps/v8app/src/favicon.ico",
              "apps/v8app/src/assets"
            ],
            "styles": [
              "apps/v8app/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/v8app/src/environments/environment.ts",
                  "with": "apps/v8app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "v8app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "v8app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "v8app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/v8app/src/test.ts",
            "polyfills": "apps/v8app/src/polyfills.ts",
            "tsConfig": "apps/v8app/tsconfig.spec.json",
            "karmaConfig": "apps/v8app/karma.conf.js",
            "assets": [
              "apps/v8app/src/favicon.ico",
              "apps/v8app/src/assets"
            ],
            "styles": [
              "apps/v8app/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/v8app/tsconfig.app.json",
              "apps/v8app/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/v8app-server",
            "main": "apps/v8app/src/main.server.ts",
            "tsConfig": "apps/v8app/tsconfig.server.json"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/v8app/src/environments/environment.ts",
                  "with": "apps/v8app/src/environments/environment.prod.ts"
                }
              ],
              "sourceMap": false,
              "optimization": {
                "scripts": false,
                "styles": true
              }
            }
          }
        }
      }
    },
    "v8app-e2e": {
      "projectType": "application",
      "root": "apps/v8app-e2e",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/v8app-e2e/protractor.conf.js",
            "devServerTarget": "v8app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "v8app:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/v8app-e2e/tsconfig.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }},
  "defaultProject": "v8app"
}